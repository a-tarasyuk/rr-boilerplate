{
  "name": "rr-boilerplate",
  "description": "React&Redux boilerplate",
  "author": "Alexander Tarasyuk",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/rr-boilerplate"
  },
  "scripts": {
    "clean": "node_modules/.bin/rimraf build/",
    "build": "npm run clean && NODE_ENV=production node_modules/.bin/webpack --config webpack/prod.config.babel.js --progress",
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config webpack/dev.config.babel.js --progress",
    "start:production": "npm run build && node_modules/.bin/http-server build/ -p 9999 -o",
    "deploy:pages": "bash .deploy"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "caniuse-db": "^1.0.30000652",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-server": "^0.10.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.1",
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.6.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-config": "^1.0.0-beta.3",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.5",
    "redux-thunk": "^2.2.0",
    "stylelint": "^7.10.1"
  }
}
