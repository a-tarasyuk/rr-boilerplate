{
  "name": "rr-boilerplate",
  "description": "React&Redux boilerplate",
  "author": "Alexander Tarasyuk",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/rr-boilerplate"
  },
  "scripts": {
    "clean": "node_modules/.bin/rimraf build/",
    "build": "npm run clean && NODE_ENV=production node_modules/.bin/webpack --config webpack/prod.config.babel.js --progress",
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config webpack/dev.config.babel.js --progress",
    "start:prod": "npm run build && node_modules/.bin/http-server build/ -p 9999 -o"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "caniuse-db": "1.0.30000810",
    "compression-webpack-plugin": "1.1.7",
    "css-loader": "0.28.9",
    "eslint": "4.18.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.7",
    "html-webpack-plugin": "2.30.1",
    "http-server": "0.11.1",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.0",
    "postcss-nested": "3.0.0",
    "rimraf": "2.6.2",
    "style-loader": "0.20.2",
    "stylelint": "9.0.0",
    "stylelint-config-standard": "18.1.0",
    "stylelint-webpack-plugin": "0.10.2",
    "thread-loader": "1.1.2",
    "uglifyjs-webpack-plugin": "1.2.0",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "history": "4.7.2",
    "normalize.css": "8.0.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
