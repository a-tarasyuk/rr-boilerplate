{
  "name": "rr-boilerplate",
  "description": "React&Redux boilerplate",
  "author": "Alexander Tarasyuk",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/a-tarasyuk/rr-boilerplate"
  },
  "scripts": {
    "clean": "node_modules/.bin/rimraf build/",
    "build": "npm run clean && NODE_ENV=production node_modules/.bin/webpack --config webpack/prod.config.babel.js --progress",
    "start": "NODE_ENV=development node_modules/.bin/webpack-dev-server --config webpack/dev.config.babel.js --progress",
    "start:production": "npm run build && node_modules/.bin/http-server build/ -p 9999 -o",
    "deploy:pages": "bash .deploy"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.26.0",
    "http-server": "^0.9.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.1",
    "stylelint-config-standard": "^15.0.1",
    "stylelint-webpack-plugin": "^0.5.1",
    "webpack": "^2.2.0-rc.7",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-merge": "^2.4.0"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "classnames": "^2.2.5",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.1",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "stylelint": "^7.6.0"
  }
}
