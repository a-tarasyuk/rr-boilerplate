    "webpack-dev-server": "^2.1.0-beta.12",
    "webpack-merge": "^1.0.2"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "classnames": "^2.2.5",
    "normalize.css": "^5.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "stylelint": "^7.6.0"
  }
